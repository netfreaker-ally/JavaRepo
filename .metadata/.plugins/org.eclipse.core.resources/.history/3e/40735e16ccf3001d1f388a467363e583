package Day1;
import java.util.*;
class Test{
	int empId;
	String eName;//It is good practice and have to declare variable
	String contact;//with small letters and when two words are combined
	String emailId;//then second word should start with big letter
	int test1;
	int test2;
	void Accept(int eid,String en,String con,String em,int t1,int t2 ) {
		empId=eid;
		eName=en;
		contact=con;
		test1=t1;
		test2=t2;
		
	}
}

public class EncapsulationEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		Test T=new Test();
		Test T1=new Test();
		//System.out.println("Enter empid:");
		int eid,t1,t2;
		String en,con,em;
		eid=sc.nextInt();
		sc.nextLine();
		en=sc.nextLine();
		em=sc.nextLine();
		con=sc.nextLine();
		t1=sc.nextInt();
		t2=sc.nextInt();
		T.Accept(eid, en, con,em, t1, t2);
		System.out.println(T.empId);
		System.out.println(T.eName);
		System.out.println(T.contact);
		System.out.println(T.test1);
		System.out.println(T.test2);
	}

}
