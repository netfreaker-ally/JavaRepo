package Day2;
import java.util.*;
public class Contact {
private	String name;
private	String Company ;
private	String Title;
private	String mobile;
private	String alternateMobile;
private	String email;
	public String Accept(String name,String Company,String Title,String mobile,String alternateMobile,String email) {
		name=name;
		Company=Company;
		Title=Title;
		mobile=mobile;
		alternateMobile=alternateMobile;
		email=email;
		//System.out.println("Name: "+name+ "\nCompany: "+Company+"\nTitle: "+Title+"\nMobile: "+mobile+"\nAlternate Contact: "+alternateMobile+"Email: "+email);
		String str="Name: "+name+ "\nCompany: "+Company+"\nTitle: "+Title+"\nMobile: "+mobile+"\nAlternate Contact: "+alternateMobile+"Email: "+email;
		return str;
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Contact c=new Contact();
		Scanner sc =new Scanner(System.in);
		System.out.println("Enter name: ");
		String nm=sc.nextLine();
		System.out.println("Enter Company: ");
		String comp=sc.nextLine();
		System.out.println("Enter Title: ");
		String tit=sc.nextLine();
		System.out.println("Enter mobile: ");
		String mob=sc.nextLine();
		System.out.println("Enter Alternate contact: ");
		String alt=sc.nextLine();
		System.out.println("Enter Email: ");
		String email=sc.nextLine();
		System.out.println(c.Accept(nm, comp, tit, mob, alt, email));
		

	}

}
